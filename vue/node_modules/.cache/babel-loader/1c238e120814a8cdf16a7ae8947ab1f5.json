OAXA;AAYA,MAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA;AACA;AACA,OAJA;AAKA,KAnBA;AAoBA,IAAA,YApBA,wBAoBA,IApBA,EAoBA;AACA,UAAA,IAAA,KAAA,CAAA,EAAA,OAAA,KAAA;AACA,UAAA,IAAA,KAAA,CAAA,EAAA,OAAA,KAAA;AACA,UAAA,IAAA,KAAA,CAAA,EAAA,OAAA,KAAA;AACA,UAAA,IAAA,KAAA,CAAA,EAAA,OAAA,KAAA;AACA,UAAA,IAAA,KAAA,CAAA,EAAA,OAAA,KAAA;AACA,KA1BA;AA2BA,IAAA,gBA3BA,4BA2BA,QA3BA,EA2BA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KA/BA;AAgCA,IAAA,mBAhCA,+BAgCA,OAhCA,EAgCA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KApCA;AAqCA,IAAA,KArCA,mBAqCA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA;AAxCA;AAhBA,CAAA","sourcesContent":["<template>\r\n<div>\r\n  <el-card>\r\n      <div>\r\n          <div style=\"margin: 10px 0\">\r\n              <el-select v-model=\"courseId\" placeholder=\"请选择课程\" >\r\n                  <el-option\r\n                          v-for=\"item in courses\"\r\n                          :key=\"item.id\" :label=\"item.name\" :value=\"item.id\">\r\n                  </el-option>\r\n              </el-select>\r\n              <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n              <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n          </div>\r\n      </div>\r\n     <div v-for=\"item in wrong\" :key=\"item.id\">\r\n       <div>学生答案：{{item.wrong.studentAnswer}}</div>\r\n       <div>学生得分：{{item.wrong.studentScore}}</div>\r\n       <div>题干：{{item.question.name}}</div>\r\n       <div>题型：{{questionType(item.question.type)}}</div>\r\n       <div>题目分数：{{item.question.score}}</div>\r\n       <div>题目解析：{{item.question.detial}}</div>\r\n       <div>题目答案：{{item.question.answer}}</div>\r\n       <el-divider></el-divider>\r\n     </div>\r\n      <div style=\"padding: 10px 0\">\r\n          <el-pagination\r\n                  @size-change=\"handleSizeChange\"\r\n                  @current-change=\"handleCurrentChange\"\r\n                  :current-page=\"pageNum\"\r\n                  :page-sizes=\"[2, 5, 10, 20]\"\r\n                  :page-size=\"pageSize\"\r\n                  layout=\"total, sizes, prev, pager, next, jumper\"\r\n                  :total=\"total\">\r\n          </el-pagination>\r\n      </div>\r\n  </el-card>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  data(){\r\n    return{\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      paperList:[],\r\n      wrong:[],\r\n        total: 0,\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        courses:[],\r\n        courseId:'',\r\n    }\r\n  },\r\n  created() {\r\n   this.load()\r\n  },\r\n  methods:{\r\n      load(){\r\n          request.get('/wrong/'+this.user.id,{\r\n              params:{\r\n                  pageNum: this.pageNum,\r\n                  pageSize: this.pageSize,\r\n                  courseId: this.courseId\r\n              }\r\n          }).then(res =>{\r\n              if (res.code ==='200'){\r\n                  this.wrong = res.data.data\r\n                  this.total = res.data.total\r\n              }\r\n          })\r\n          request.get(\"/course\").then(res=>{\r\n               if (res.code == 200){\r\n                   this.courses = res.data\r\n               }\r\n          })\r\n      },\r\n    questionType(type){\r\n      if (type === 1) return '选择题'\r\n      if (type === 2) return '判断题'\r\n      if (type === 3) return '问答题'\r\n      if (type === 4) return '多选题'\r\n      if (type === 5) return '填空题'\r\n    },\r\n      handleSizeChange(pageSize) {\r\n          console.log(pageSize)\r\n          this.pageSize = pageSize\r\n          this.load()\r\n      },\r\n      handleCurrentChange(pageNum) {\r\n          console.log(pageNum)\r\n          this.pageNum = pageNum\r\n          this.load()\r\n      },\r\n      reset() {\r\n          this.courseId = \"\"\r\n          this.load()\r\n      },\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/views/front"}]}