AA,KAAA,CAAA,SAAA;AACA;AACA,OAZA;AAaA,MAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA;AACA;AACA,OAJA;AAKA,KApBA;AAqBA,IAAA,SArBA,qBAqBA,EArBA,EAqBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,OADA;AAEA,QAAA,KAAA,EAAA;AACA,UAAA,cAAA,EAAA,EADA;AAEA,UAAA,IAAA,EAAA;AAFA;AAFA,OAAA;AAOA,KA9BA;AA+BA,IAAA,gBA/BA,4BA+BA,QA/BA,EA+BA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KAnCA;AAoCA,IAAA,mBApCA,+BAoCA,OApCA,EAoCA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KAxCA;AAyCA,IAAA,KAzCA,mBAyCA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA;AA5CA;AAfA,CAAA","sourcesContent":["<template>\r\n<div>\r\n  <el-card >\r\n      <div>\r\n          <div style=\"margin: 10px 0\">\r\n              <el-select v-model=\"courseId\" placeholder=\"请选择课程\" >\r\n                  <el-option\r\n                          v-for=\"item in courses\"\r\n                          :key=\"item.id\" :label=\"item.name\" :value=\"item.id\">\r\n                  </el-option>\r\n              </el-select>\r\n              <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n              <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n          </div>\r\n      </div>\r\n     <div v-for=\"item in paperList\">\r\n       <div>考试id：{{item.id}}</div>\r\n       <div>试卷名称：{{item.examName}}</div>\r\n       <div>课程名称：{{item.courseName}}</div>\r\n       <div>考试时间：{{item.time}}</div>\r\n       <div>考试状态：{{item.status}}</div>\r\n       <div>\r\n         <el-button @click=\"viewPaper(item.id)\">查看试卷</el-button>\r\n       </div>\r\n     </div>\r\n      <div style=\"padding: 10px 0\">\r\n          <el-pagination\r\n                  @size-change=\"handleSizeChange\"\r\n                  @current-change=\"handleCurrentChange\"\r\n                  :current-page=\"pageNum\"\r\n                  :page-sizes=\"[2, 5, 10, 20]\"\r\n                  :page-size=\"pageSize\"\r\n                  layout=\"total, sizes, prev, pager, next, jumper\"\r\n                  :total=\"total\">\r\n          </el-pagination>\r\n      </div>\r\n  </el-card>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n   data(){\r\n     return{\r\n       user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n       paperList:[],\r\n         total: 0,\r\n         pageNum: 1,\r\n         pageSize: 10,\r\n         courses:[],\r\n         courseId:'',\r\n     }\r\n },\r\n  created() {\r\n     this.load()\r\n  },\r\n  methods:{\r\n       load(){\r\n           request.get(\"/studentPaper/user/\"+this.user.id,{\r\n               params:{\r\n                   pageNum: this.pageNum,\r\n                   pageSize: this.pageSize,\r\n                   courseId: this.courseId\r\n               }\r\n           }).then(res =>{\r\n               if (res.code ==='200'){\r\n                   this.paperList = res.data.data\r\n                   this.total = res.data.total\r\n                   console.log(\"考试记录：\"+ this.paperList)\r\n               }\r\n           })\r\n           request.get(\"/course\").then(res=>{\r\n               if (res.code == 200){\r\n                   this.courses = res.data\r\n               }\r\n           })\r\n       },\r\n     viewPaper(id){\r\n       console.log(\"id\"+id)\r\n       this.$router.push({\r\n         name:'paper',\r\n         query:{\r\n           studentPaperId: id,\r\n            type: 2\r\n         }\r\n       })\r\n     },\r\n      handleSizeChange(pageSize) {\r\n          console.log(pageSize)\r\n          this.pageSize = pageSize\r\n          this.load()\r\n      },\r\n      handleCurrentChange(pageNum) {\r\n          console.log(pageNum)\r\n          this.pageNum = pageNum\r\n          this.load()\r\n      },\r\n      reset() {\r\n          this.courseId = \"\"\r\n          this.load()\r\n      },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/views/front"}]}