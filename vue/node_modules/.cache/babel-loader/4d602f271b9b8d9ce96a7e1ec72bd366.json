ur = hour < 10 ? \"0\" + hour : hour;\r\n          min = min < 10 ? \"0\" + min : min;\r\n          sec = sec < 10 ? \"0\" + sec : sec;\r\n          let format = '';\r\n          if(day > 0){\r\n            format = `${day}天${hour}小时${min}分${sec}秒`;\r\n          }\r\n          if(day <= 0 && hour > 0 ){\r\n            format = `${hour}小时${min}分${sec}秒`;\r\n          }\r\n          if(day <= 0 && hour <= 0){\r\n            format =`${min}分${sec}秒`;\r\n          }\r\n          self.content = format;\r\n        }else{\r\n          clearInterval(timer);\r\n          self.content = self.endText;\r\n          self._callback();\r\n        }\r\n      },1000);\r\n    },\r\n    _callback(){\r\n      if(this.callback && this.callback instanceof Function){\r\n        this.callback(...this);\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/views/front"}]}