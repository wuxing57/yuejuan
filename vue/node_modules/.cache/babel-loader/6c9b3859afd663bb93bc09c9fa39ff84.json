A,iBAAA,GAAA,IAAA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,UAAA;AACA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,UAAA;AACA;AACA,OAPA;AAQA,KA1EA;AA2EA,IAAA,UA3EA,sBA2EA,GA3EA,EA2EA;AAAA;;AACA,WAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA,CAAA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,UAAA;AACA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,UAAA;AACA;AACA,OAPA;AAQA,KAtFA;AAuFA,IAAA,GAvFA,eAuFA,EAvFA,EAuFA;AAAA;;AACA,WAAA,OAAA,CAAA,MAAA,CAAA,YAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OAPA;AAQA,KAhGA;AAiGA,IAAA,qBAjGA,iCAiGA,GAjGA,EAiGA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAAA,iBAAA,GAAA,GAAA;AACA,KApGA;AAqGA,IAAA,QArGA,sBAqGA;AAAA;;AACA,UAAA,CAAA,KAAA,iBAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA;;AACA,UAAA,GAAA,GAAA,KAAA,iBAAA,CAAA,GAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA;AAAA,OAAA,CAAA,CALA,CAKA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,kBAAA,EAAA,GAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA;AACA,OAPA;AAQA,KAnHA;AAoHA,IAAA,KApHA,mBAoHA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA,KAvHA;AAwHA,IAAA,gBAxHA,4BAwHA,QAxHA,EAwHA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KA5HA;AA6HA,IAAA,mBA7HA,+BA6HA,OA7HA,EA6HA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KAjIA;AAkIA,IAAA,uBAlIA,mCAkIA,GAlIA,EAkIA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,GAAA;AACA,KApIA;AAqIA,IAAA,sBArIA,kCAqIA,GArIA,EAqIA;AACA,WAAA,IAAA,CAAA,GAAA,GAAA,GAAA;AACA,KAvIA;AAwIA,IAAA,QAxIA,oBAwIA,GAxIA,EAwIA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,GAAA;AACA,KA1IA;AA2IA,IAAA,GA3IA,iBA2IA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,oCAAA;AACA,KA7IA;AA8IA,IAAA,wBA9IA,sCA8IA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,WAAA,IAAA;AACA;AAjJA;AA1BA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"name\"></el-input>\r\n<!--      <el-input style=\"width: 200px\" placeholder=\"请输入\" suffix-icon=\"el-icon-message\" class=\"ml-5\" v-model=\"email\"></el-input>-->\r\n<!--      <el-input style=\"width: 200px\" placeholder=\"请输入\" suffix-icon=\"el-icon-position\" class=\"ml-5\" v-model=\"address\"></el-input>-->\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n\r\n      <el-button type=\"primary\" @click=\"handleAdd\">新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我再想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定批量删除这些数据吗？\"\r\n          @confirm=\"delBatch\"\r\n      >\r\n        <el-button type=\"danger\" slot=\"reference\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n      <!-- <el-upload action=\"http://localhost:9090/paper/import\" :show-file-list=\"false\" accept=\"xlsx\" :on-success=\"handleExcelImportSuccess\" style=\"display: inline-block\">\r\n        <el-button type=\"primary\" class=\"ml-5\">导入 <i class=\"el-icon-bottom\"></i></el-button>\r\n      </el-upload>\r\n      <el-button type=\"primary\" @click=\"exp\" class=\"ml-5\">导出 <i class=\"el-icon-top\"></i></el-button> -->\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\"  @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\" sortable></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"试卷名称\"></el-table-column>\r\n      <el-table-column prop=\"score\" label=\"试卷总分\"></el-table-column>\r\n      <el-table-column prop=\"duration\" label=\"考试时长\"></el-table-column>\r\n      <el-table-column  label=\"所属课程\">\r\n        <template v-slot=\"scope\">\r\n          <span>{{courses.find(v => v.id===scope.row.courseId).name}}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column  label=\"查看试卷\">\r\n        <template v-slot=\"scope\">\r\n        <el-button type=\"success\" @click=\"viewPaper(scope.row.id)\">查看<i class=\"el-icon-edit\"></i></el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\"  width=\"380\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" @click=\"handleAutoPaper(scope.row.id,scope.row.courseId)\">自动组卷 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n          <el-button type=\"primary\" @click=\"handlePaper(scope.row.id,scope.row.courseId)\">手动组卷 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"40%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"120px\" size=\"small\" style=\"width: 80%; margin: 0 auto\">\r\n        <el-form-item label=\"试卷名称\">\r\n          <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"试卷总分\">\r\n          <el-input v-model=\"form.score\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"考试时长\">\r\n          <el-input v-model=\"form.duration\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"课程\">\r\n          <el-select v-model=\"form.courseId\" placeholder=\"请选择\">\r\n            <el-option\r\n                v-for=\"item in courses\"\r\n                :key=\"item.id\" :label=\"item.name\" :value=\"item.id\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible1\" width=\"40%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"120px\" size=\"small\" style=\"width: 80%; margin: 0 auto\">\r\n        <el-form-item label=\"选择题\">\r\n          <el-input v-model=\"form1.type1\" autocomplete=\"off\" ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"填空题\">\r\n          <el-input v-model=\"form1.type2\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"问答题\">\r\n          <el-input v-model=\"form1.type3\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"generatorPaper\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog title=\"试卷信息\" :visible.sync=\"dialogFormVisible2\" width=\"40%\" :close-on-click-modal=\"false\">\r\n      <div>\r\n        <div v-for=\"(item,index) in papers \" :key=\"papers.id\" >\r\n          <div>\r\n            {{index+1}}.{{item.name}}\r\n            <span v-if=\"item.type===1\">(选择题)</span>\r\n            <span v-if=\"item.type===2\">(填空题)</span>\r\n            <span v-if=\"item.type===3\">(问答题)</span>\r\n          </div>\r\n          <div v-if=\"item.type===1\">\r\n            <span>A.{{item.a}}</span>\r\n            <span>B.{{item.b}}</span>\r\n            <span>C.{{item.c}}</span>\r\n            <span>D.{{item.d}}</span>\r\n          </div>\r\n            <div>答案：{{item.answer}}</div>\r\n            <div>解析：{{item.detial}}</div>\r\n        </div>\r\n      </div>\r\n    </el-dialog>\r\n    <el-dialog title=\"手动组卷\" :visible.sync=\"dialogFormVisible3\" width=\"40%\" :close-on-click-modal=\"false\">\r\n\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Paper\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      name: \"\",\r\n      form: {},\r\n      form1: {},\r\n      form2: {},\r\n      dialogFormVisible: false,\r\n      dialogFormVisible1: false,\r\n      dialogFormVisible2: false,\r\n      dialogFormVisible3: false,\r\n      multipleSelection: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      courses:[],\r\n      papers:[],\r\n      questions:[]\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      this.request.get(\"/paper/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      }),\r\n          this.request.get(\"/course\").then(res => {\r\n            this.courses=res.data\r\n          })\r\n    },\r\n    generatorPaper(){\r\n      this.request.post(\"/paper/generatorPaper\", this.form1).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"组卷成功\")\r\n          this.dialogFormVisible1 = false\r\n        } else {\r\n          this.$message.error(\"组卷失败\")\r\n        }\r\n      })\r\n    },\r\n    viewPaper(paperId){\r\n      this.request.get(\"/paper/view/\"+paperId).then(res => {\r\n        if (res.code === '200') {\r\n          this.i = 0;\r\n          this.$message.success(\"查看试卷成功\")\r\n          this.dialogFormVisible2 = true\r\n          this.papers=res.data\r\n        } else {\r\n          this.$message.error(\"查看试卷失败\")\r\n        }\r\n      })\r\n    },\r\n    save() {\r\n      this.request.post(\"/paper\", this.form).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"保存成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n      })\r\n    },\r\n    handleAutoPaper(paperId,courseId){\r\n      this.form1={type1:4,type2:4,type3:2,paperId:paperId,courseId:courseId}\r\n      this.dialogFormVisible1 = true\r\n    },\r\n    handlePaper(paperId,courseId){\r\n     // this.form2={type1:4,type2:4,type3:2,paperId:paperId,courseId:courseId}\r\n      this.dialogFormVisible3 = true\r\n     this.request(\"/question\").then(res=>{\r\n       if (res.code==='200'){\r\n         this.questions=res.data\r\n       }\r\n     })\r\n\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n      this.$nextTick(() => {\r\n        if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n       this.$nextTick(() => {\r\n         if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n       })\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/paper/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      if (!this.multipleSelection.length) {\r\n        this.$message.error(\"请选择需要删除的数据\")\r\n        return\r\n      }\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"/paper/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    handleFileUploadSuccess(res) {\r\n      this.form.file = res\r\n    },\r\n    handleImgUploadSuccess(res) {\r\n      this.form.img = res\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n    exp() {\r\n      window.open(\"http://localhost:9090/paper/export\")\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.headerBg {\r\n  background: #eee!important;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}