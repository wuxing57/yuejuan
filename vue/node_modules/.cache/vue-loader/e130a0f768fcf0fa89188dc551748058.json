      v-for=\"item in [{name:'选择题',value:1},{name:'判断题',value:2},{name:'问答题',value:3},]\"\r\n            :key=\"item.value\" :label=\"item.name\" :value=\"item.value\">\r\n        </el-option>\r\n      </el-select>\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n  <el-button @click=\"addQuestion\">添加</el-button>\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\"  @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\" sortable></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"题干\"></el-table-column>\r\n      <el-table-column  label=\"所属课程\">\r\n        <template v-slot=\"scope\">\r\n          <span>{{courses.find(v => v.id===scope.row.courseId).name}}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"type\" label=\"类型\">\r\n        <template v-slot=\"scope\">\r\n          <span v-if=\"scope.row.type===1\" >选择题</span>\r\n          <span v-if=\"scope.row.type===2\">判断题</span>\r\n          <span v-if=\"scope.row.type===3\">问答题</span>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport th from \"element-ui/src/locale/lang/th\";\r\nimport Bus from \"@/bus/bus\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      multipleSelection:[],\r\n      name: \"\",\r\n      type: \"\",\r\n      courseId: \"\",\r\n      paperId:'',\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      courses:[],\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n    this.request.get(\"/course\").then(res => {\r\n      this.courses=res.data\r\n    })\r\n    this.getPaperId()\r\n  },\r\n\r\n  methods:{\r\n    getPaperId(){\r\n      if (this.$store.getters.paperId !== ''){\r\n        this.paperId = this.$store.getters.paperId\r\n      }\r\n       console.log(\"list:\"+this.paperId)\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    load() {\r\n      this.request.get(\"/question/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n          type: this.type,\r\n          courseId: this.courseId,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.type = ''\r\n      this.courseId = ''\r\n      this.load()\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    addQuestion() {\r\n      if (!this.multipleSelection.length) {\r\n        this.$message.error(\"请选择需要添加的数据\")\r\n        return\r\n      }\r\n      let param =[]\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n       ids.forEach(questionId =>{\r\n            param.push({\"paperId\":this.paperId,\"QuestionId\":questionId})\r\n       })\r\n      let list = JSON.stringify(param)\r\n      console.log(list)\r\n      this.request.post(\"/paperQuestion/all\",list).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"添加成功\")\r\n          this.load()\r\n\r\n        } else {\r\n          this.$message.error(\"添加失败\")\r\n        }\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}